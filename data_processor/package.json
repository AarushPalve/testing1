{
  "name": "frc-match-predictor",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "electron .",
    "build:win": "vite build && electron-builder --win portable"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "electron": "^33.0.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  },
  "build": {
    "appId": "com.frc.predictor",
    "productName": "FRC Match Predictor Pro",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "main.cjs"
    ],
    "win": {
      "target": "portable",
      "requestedExecutionLevel": "asInvoker"
    }
  }
}
